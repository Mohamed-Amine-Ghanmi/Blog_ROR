<h1>Edit Moderator</h1>

<p><%= render 'validation_errors', object: @moderator %></p>

<%= form_for [:admin, @moderator] do |f| %>
	
	<p>
	<%= f.label :Fullname %>
	<%= f.text_field :fullname %>
	</p>

	<p>
	<%= f.label :Username %>
	<%= f.text_field :username %>
	</p>

	<p>
	<%= f.label :Password %>
	<%= f.password_field  :password %>
	</p>

	<%= f.submit %>

<% end %>